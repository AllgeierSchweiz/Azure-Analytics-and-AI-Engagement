{
    "name": "6 FootTraffic IoT Realtime PARQUET Data Migration",
    "properties": {
        "activities": [
            {
                "name": "IoT PARQUET Data Migration",
                "type": "Copy",
                "dependsOn": [],
                "policy": {
                    "timeout": "7.00:00:00",
                    "retry": 0,
                    "retryIntervalInSeconds": 30,
                    "secureOutput": false,
                    "secureInput": false
                },
                "userProperties": [],
                "typeProperties": {
                    "source": {
                        "type": "ParquetSource",
                        "storeSettings": {
                            "type": "AzureBlobFSReadSettings",
                            "recursive": true,
                            "modifiedDatetimeStart": {
                                "value": "@getPastTime(10, 'minute')",
                                "type": "Expression"
                            },
                            "wildcardFileName": "*.*",
                            "enablePartitionDiscovery": false
                        }
                    },
                    "sink": {
                        "type": "SqlDWSink",
                        "tableOption": "autoCreate",
                        "disableMetricsCollection": false
                    },
                    "enableStaging": false,
                    "translator": {
                        "type": "TabularTranslator",
                        "mappings": [
                            {
                                "source": {
                                    "name": "RecordedOn",
                                    "type": "String",
                                    "physicalType": "UTF8"
                                },
                                "sink": {
                                    "name": "RecordedOn",
                                    "type": "String",
                                    "physicalType": "nvarchar"
                                }
                            },
                            {
                                "source": {
                                    "name": "before-foottraffic",
                                    "type": "Int64",
                                    "physicalType": "INT64"
                                },
                                "sink": {
                                    "name": "before-foottraffic",
                                    "type": "Int64",
                                    "physicalType": "bigint"
                                }
                            },
                            {
                                "source": {
                                    "name": "after-foottraffic",
                                    "type": "Int64",
                                    "physicalType": "INT64"
                                },
                                "sink": {
                                    "name": "after-foottraffic",
                                    "type": "Int64",
                                    "physicalType": "bigint"
                                }
                            },
                            {
                                "source": {
                                    "name": "EventProcessedUtcTime",
                                    "type": "DateTime",
                                    "physicalType": "INT96"
                                },
                                "sink": {
                                    "name": "EventProcessedUtcTime",
                                    "type": "DateTime",
                                    "physicalType": "datetime2"
                                }
                            },
                            {
                                "source": {
                                    "name": "PartitionId",
                                    "type": "Int64",
                                    "physicalType": "INT64"
                                },
                                "sink": {
                                    "name": "PartitionId",
                                    "type": "Int64",
                                    "physicalType": "bigint"
                                }
                            },
                            {
                                "source": {
                                    "name": "EventEnqueuedUtcTime",
                                    "type": "DateTime",
                                    "physicalType": "INT96"
                                },
                                "sink": {
                                    "name": "EventEnqueuedUtcTime",
                                    "type": "DateTime",
                                    "physicalType": "datetime2"
                                }
                            }
                        ],
                        "typeConversion": true,
                        "typeConversionSettings": {
                            "allowDataTruncation": true,
                            "treatBooleanAsNumber": false
                        }
                    }
                },
                "inputs": [
                    {
                        "referenceName": "IOTParquetToSynapseSource",
                        "type": "DatasetReference"
                    }
                ],
                "outputs": [
                    {
                        "referenceName": "AzureSynapseAnalyticsTable2",
                        "type": "DatasetReference"
                    }
                ]
            }
        ],
        "annotations": [],
        "lastPublishTime": "2022-01-05T20:12:52Z"
    },
    "type": "Microsoft.Synapse/workspaces/pipelines"
}